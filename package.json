{
  "name": "tree-house-translations",
  "version": "1.1.3",
  "description": "Import translations from iCapps translation service",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib/*",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/source/cli.js --no-cleanup"
  },
  "bin": {
    "translations": "./bin/translations"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "commander": "~2.20.0",
    "del": "~3.0.0",
    "inquirer": "~6.2.2",
    "lodash": "~4.17.11",
    "mkdirp": "~0.5.1",
    "node-fetch": "~2.3.0",
    "plist": "~3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/mkdirp": "~0.5.2",
    "@types/nock": "^9.3.1",
    "coveralls": "^3.0.2",
    "jest": "^24.0.0",
    "nock": "^10.0.6",
    "np": "^4.0.2",
    "rimraf": "^2.6.3",
    "ts-jest": "~23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icapps/tree-house-translations.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-translations/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-translations#readme"
}
