{
  "name": "tree-house-translations",
  "version": "1.0.0",
  "description": "Import translations from iCapps translation service",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib/*",
    "build": "yarn clean && tsc",
    "start": "yarn build && node lib/index.js",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll",
    "pretest": "yarn build",
    "prepublish": "yarn build",
    "release": "node_modules/np/cli.js --no-cleanup"
  },
  "bin": {
    "translations": "./bin/translations"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "commander": "~2.15.1",
    "del": "~3.0.0",
    "gulp": "~3.9.0",
    "inquirer": "~5.2.0",
    "lodash": "~4.17.10",
    "mkdirp": "~0.5.1",
    "node-fetch": "~2.1.2",
    "plist": "~3.0.1"
  },
  "devDependencies": {
    "@types/jest": "~22.2.3",
    "@types/mkdirp": "^0.5.2",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.7",
    "auditjs": "^2.4.3",
    "coveralls": "^3.0.1",
    "jest": "^22.4.3",
    "nock": "^9.2.5",
    "np": "^2.20.1",
    "rimraf": "^2.6.2",
    "ts-jest": "~22.4.4",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=0.12.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icapps/tree-house-translations.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-translations/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-translations#readme"
}
